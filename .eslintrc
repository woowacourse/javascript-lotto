{
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "parser": "babel-eslint",
  "extends": "airbnb-base",
  "env": {
    "browser": true,
    "node": true,
    "mocha": true
  },
  "rules": {
    "max-length": 0, // 0으로 설정하면 에러 검출 X
    "comma-dangle": ["error", "always-multiline"],
  },
  "plugins": ["import"],
  "globals": {
    "jQuery": true,
    "$": true
  }
}
